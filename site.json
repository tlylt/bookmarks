{
  "baseUrl": "/bookmarks",
  "titlePrefix": "tlylt",
  "faviconPath": "favicon.ico",
  "style": {
    "bootstrapTheme": "bootswatch-litera",
    "codeTheme": "light"
  },
  "plugins": [
    "codeBlockCopyButtons"
  ],
  "ignore": [
    "_markbind/layouts/*",
    "_markbind/logs/*",
    "_site/*",
    "site.json",
    "*.md",
    "*.njk",
    ".git/*",
    "node_modules/*"
  ],
  "pagesExclude": [
    "node_modules/*"
  ],
  "pages": [
    {
      "src": "index.md",
      "title": "Bookmarks"
    },
    {
      "src": "contents/productivity.md",
      "title": "Productivity"
    },
    {
      "src": "contents/patterns.md",
      "title": "Patterns"
    },
    {
      "src": "contents/golang.md",
      "title": "Golang"
    },
    {
      "src": "contents/review.md",
      "title": "Code Review"
    },
    {
      "src": "contents/conversational.md",
      "title": "Conversational Agent"
    },
    {
      "src": "contents/research.md",
      "title": "Research Notes"
    },
    {
      "src": "contents/interviewing.md",
      "title": "My Coding Interview Notes"
    },
    {
      "glob": [
        "**/index.md",
        "**/*.md"
      ]
    }
  ],
  "deploy": {
    "message": "Site Update."
  }
}